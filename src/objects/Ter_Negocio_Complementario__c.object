    <validationRules>
        <fullName>Fielo_NNCC_RestringirActualizacionName</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 $Profile.Name = &apos;Fielo Backend User&apos;
,ISCHANGED(Name) 
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Tu perfil no permite la edición del campo Nombre</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fielo_TTCC_IdFidelizacionRequerido</fullName>
        <active>true</active>
        <errorConditionFormula>F_ClubTerpel__c &amp;&amp; ISBLANK(F_IdFidelizacion__c)</errorConditionFormula>
        <errorDisplayField>F_IdFidelizacion__c</errorDisplayField>
        <errorMessage>Para poder habilitar el punto de venta en Club Terpel el campo Id Fidelización es Requerido</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TER_NCOM_001_Valida_propietario</fullName>
        <active>true</active>
        <description>Esta regla valida que el campo propietario no se modifique por algunos perfiles.</description>
        <errorConditionFormula>AND( 
ISCHANGED(OwnerId), 
$Profile.Name &lt;&gt; &quot;Administrador del sistema&quot;, 
$Profile.Name &lt;&gt; &quot;Administrador Funcional Plataforma&quot;, 
$Profile.Name &lt;&gt; &quot;Administrador Técnico Plataforma&quot;, 
$Profile.Name &lt;&gt; &quot;Administrador Funcional Comercial&quot;, 
$Profile.Name &lt;&gt; &quot;Cartera&quot; 
)</errorConditionFormula>
        <errorMessage>TER-NCOM-001: Su perfil no permite modificar el propietario.</errorMessage>
    </validationRules>
