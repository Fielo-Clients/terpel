    <webLinks>
        <fullName>Fielo_ReintentarTransaccionLifeMiles</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reintentar Transacción LifeMiles</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 

var confirmacao = confirm(&apos;Usted está seguro de que desea realizar esta operación?&apos;); 

if (confirmacao == true) { 
var lAutorizacionID = &apos;{!Fielo_Autorizacion_LifeMiles__c.Id}&apos;;
var result = sforce.apex.execute(&apos;Fielo_UTIL_MillasLifeMiles&apos;,&apos;reintentarseMillasLifeMiles&apos;,{aIdAutLifeMiles:lAutorizacionID }); 

alert(result); 
window.location.reload(); 
} 
else { 
alert(&apos;Operação Cancelada&apos;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>Fielo_RevertirTransaccionLifeMiles</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Revertir Transacción LifeMiles</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

var confirmacao = &apos;Desea realmente revertir esta transacción?&apos; ; 

if ( confirm( confirmacao) ){ 
var lLifeMilesID = &apos;{!Fielo_Autorizacion_LifeMiles__c.Id}&apos;; 
var lResult = new String( sforce.apex.execute( &quot;Fielo_UTIL_MillasLifeMiles&quot;,&quot;reversionMillasLifeMiles&quot;,{aIdAutLifeMiles:lLifeMilesID } ) ); 
if ( lResult.startsWith( &apos;/&apos; ) ) window.location = lResult; 
else alert( lResult );
window.location.reload(); 
}</url>
    </webLinks>
